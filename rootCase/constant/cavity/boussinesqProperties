FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      boussinesqProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

compr   false; // true for compressible simulation

// Used only if compr is set to false

// Thermal expansion coefficient
beta    [0 0 0 -1 0 0 0]    2e-04;

// Reference temperature
T0      [0 0 0 1 0 0 0]     900;

// Reference density
rho0    [1 -3 0 0 0 0 0]    2000;

