FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/neutroRegion";
    object      defaultFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -3 0 0 0 0 0 ];

internalField   uniform 1950;

boundaryField
{
    ".*Wall.*"
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


