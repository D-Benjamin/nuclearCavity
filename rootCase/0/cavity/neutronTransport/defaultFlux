FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/neutroRegion";
    object      defaultFlux;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -2 -1 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
    ".*Wall.*"
    {
        type            albedo;
        value           uniform 1;
    }
    frontAndBack
    {
        type            empty;
    }
}


